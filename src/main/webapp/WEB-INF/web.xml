<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>Hello</servlet-name>
        <servlet-class>Hello</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Hello</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Signin</servlet-name>
        <servlet-class>servlet.SigninServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Signin</servlet-name>
        <url-pattern>/SigninServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Signup</servlet-name>
        <servlet-class>servlet.SignupServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Signup</servlet-name>
        <url-pattern>/SignupServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Verify</servlet-name>
        <servlet-class>servlet.VerifyServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Verify</servlet-name>
        <url-pattern>/verify</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Recovery</servlet-name>
        <servlet-class>servlet.RecoveryServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Recovery</servlet-name>
        <url-pattern>/RecoveryServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ChangePassword</servlet-name>
        <servlet-class>servlet.ChangePasswordServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ChangePassword</servlet-name>
        <url-pattern>/ChangePassword</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SubmitFriends</servlet-name>
        <servlet-class>servlet.SubmitFriendsServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SubmitFriends</servlet-name>
        <url-pattern>/SubmitFriendsServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Signout</servlet-name>
        <servlet-class>servlet.SignoutServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Signout</servlet-name>
        <url-pattern>/signout</url-pattern>
    </servlet-mapping>
</web-app>
